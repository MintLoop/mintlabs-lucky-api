{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "astro",
  "env": {
    "BACKEND_ORIGIN": "https://mintlabs-lucky-api.vercel.app"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://mintlabs-lucky-api.vercel.app/:path*"
    }
  ]
}
