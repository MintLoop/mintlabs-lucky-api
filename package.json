{
  "name": "mintlabs-lucky-monorepo",
  "version": "0.0.1",
  "private": true,
  "description": "Monorepo wrapper - frontend is in mintlabs-lucky-frontend/",
  "scripts": {
    "dev": "cd mintlabs-lucky-frontend && npm run dev",
    "build": "cd mintlabs-lucky-frontend && npm run build",
    "build:fresh": "cd mintlabs-lucky-frontend && npm ci && npm run build",
    "preview": "cd mintlabs-lucky-frontend && npm run preview",
    "test:e2e": "cd mintlabs-lucky-frontend && npm run test:e2e",
    "lint": "cd mintlabs-lucky-frontend && npm run lint",
    "postinstall": "cd mintlabs-lucky-frontend && npm ci"
  },
  "engines": {
    "node": ">=18"
  }
}
