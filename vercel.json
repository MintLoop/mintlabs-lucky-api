{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": "mintlabs-lucky-frontend/dist",
  "installCommand": "npm install && cd mintlabs-lucky-frontend && npm ci",
  "framework": null,
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- mintlabs-lucky-frontend/",
  "env": {
    "PUBLIC_API_BASE": "@api_base_url",
    "PUBLIC_ADSENSE_CLIENT": "@adsense_client"
  }
}
